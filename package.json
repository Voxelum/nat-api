{
  "version": "0.3.1",
  "name": "@xmcl/nat-api",
  "description": "Port mapping with UPnP and NAT-PMP",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "scripts": {
    "build:cjs": "esbuild --bundle --outfile=dist/index.js --format=cjs --platform=node --external:default-gateway --external:fast-xml-parser --minify index.ts",
    "build:esm": "esbuild --bundle --outfile=dist/index.mjs --external:default-gateway --external:fast-xml-parser --format=esm --platform=node --minify index.ts",
    "build": "pnpm run build:cjs && pnpm run build:esm",
    "test": "uvu -r esbuild-register tests"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "default-gateway": "^6.0.3",
    "fast-xml-parser": "^4.0.1"
  },
  "devDependencies": {
    "esno": "^0.14.0",
    "@types/node": "^17.0.8",
    "esbuild": "^0.14.11",
    "esbuild-register": "^3.3.2",
    "typescript": "^4.5.4",
    "uvu": "^0.5.3"
  },
  "author": {
    "name": "Alex",
    "email": "alxmorais8@msn.com"
  },
  "contributors": [
    {
      "name": "CI010",
      "email": "cijhn@hotmail.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voxelum/nat-api.git"
  },
  "bugs": {
    "url": "https://github.com/voxelum/nat-api/issues"
  },
  "homepage": "https://github.com/voxelum/nat-api",
  "keywords": [
    "nat",
    "api",
    "upnp",
    "pmp",
    "nat-pmp",
    "holepunch",
    "port",
    "forwarding",
    "map",
    "mapping"
  ]
}